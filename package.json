{
  "name": "eventsapis-proto",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:event-sourcing-tutorial/eventsapis-proto.git",
  "author": "Abdulaziz Ghuloum <aghuloum@gmail.com>",
  "license": "MIT",
  "files": [
    "./src-ts",
    "./build"
  ],
  "dependencies": {
    "protobufjs": "^7.1.2"
  },
  "devDependencies": {
    "ts-proto": "^1.150.1",
    "@grpc/grpc-js": "^1.8.5",
    "typescript": "^4.9.3"
  },
  "scripts": {
    "protoc": "rm -rf ./src-ts/proto ./src-ts/google && protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_opt=oneof=unions --ts_proto_opt=stringEnums=true --ts_proto_opt=outputClientImpl=false --ts_proto_opt=forceLong=bigint --ts_proto_opt=esModuleInterop=true --ts_proto_opt=outputJsonMethods=false --ts_proto_opt=outputPartialMethods=false --ts_proto_opt=outputServices=grpc-js --ts_proto_out=./src-ts ./proto/*",
    "watch": "tsc -w",
    "build": "tsc",
    "prepack": "yarn protoc && yarn tsc",
    "start": "nodemon -r source-map-support/register --watch ./build"
  }
}
